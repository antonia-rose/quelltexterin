{
  "name": "quelltexterin",
  "version": "1.0.0",
  "description": "a minimalistic and lightweight blog",
  "main": "index.html",
  "scripts": {
    "css:build": "node-sass --recursive --output-style compressed themes/quelltexterin/sources/sass/ --output themes/quelltexterin/static/css",
    "postcss:build": "autoprefixer -b 'last 2 versions' static/css/*.css",
    "css:watch": "onchange 'themes/quelltexterin/sources/sass/' -- npm run css:build",
    "build": "npm run css:build",
    "prewatch": "npm run build",
    "watch": "npm run css:watch",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soophie/quelltexterin.git"
  },
  "author": "Antonia Ilski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soophie/quelltexterin/issues"
  },
  "homepage": "https://github.com/soophie/quelltexterin#readme",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "scss-cli": "0.0.6"
  }
}
